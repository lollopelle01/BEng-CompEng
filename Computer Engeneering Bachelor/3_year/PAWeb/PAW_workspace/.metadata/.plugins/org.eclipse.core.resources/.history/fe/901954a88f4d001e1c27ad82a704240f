	// --- CRUD ------------
	
	public void create(int idPartita, int idStadio);

	public boolean delete(int idPartita, int idStadio);
	
	// ----------------------------------

	public boolean createTable();

	public boolean dropTable();

	// Metodi di utility
	public Set<String> getNumPartitePerCategoria(int idStadio);

}
